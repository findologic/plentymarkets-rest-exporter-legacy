{
  "name": "findologic/plentymarkets-rest-export",
  "type": "library",
  "description": "FINDOLOGIC plugin for Plentymarkets",
  "keywords": [
    "Plentymarkets",
    "FINDOLOGIC"
  ],
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "^6.3|^7.1",
    "guzzlehttp/promises": "^1.4",
    "monolog/monolog": "^2.3",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "8.0.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Findologic\\Plentymarkets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Findologic\\Plentymarkets\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:vasily-kartashov/log4php.git"
    }
  ],
  "scripts": {
    "test": "XDEBUG_MODE=coverage phpunit --configuration phpunit.xml"
  }
}
